{
  "name": "web_docs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && ROLLUP_STEP=pagedjs-polyfill vite build ",
    "preview": "vite preview",
    "tauri": "tauri",
    "copy_node": "node copy_node.js",
    "prepare_pagedjs": "cd node_modules/pagedjs-cli && cp ../../pagedjs-cli/* . && npm ci",
    "_pkg_pagedjs": "(cd node_modules/pagedjs-cli; npm i); cd node_modules/pagedjs-cli; ../.bin/pkg -o pagedjs-cli src/cli.js",
    "_pkg_fetch_pagedjs": "(cd node_modules/pagedjs-cli; npm i); cd node_modules/pagedjs-cli; ../.bin/pkg-fetch -o dist src/cli.js",
    "_nexe": "(cd node_modules/pagedjs-cli; npm i); node ./nexe.cjs"
  },
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@tauri-apps/api": ">=2.0.0-beta.0",
    "@tauri-apps/plugin-cli": "^2.0.0-beta.7",
    "@tauri-apps/plugin-dialog": "^2.0.0-beta.6",
    "@tauri-apps/plugin-fs": "^2.0.0-beta.6",
    "@tauri-apps/plugin-process": "^2.0.0-beta.7",
    "@tauri-apps/plugin-shell": ">=2.0.0-beta.0",
    "@tauri-apps/plugin-updater": "^2.0.0-beta.7",
    "ace-code": "^1.35.2",
    "jodit": "^4.2.27",
    "jodit-react": "^4.1.2",
    "js-beautify": "^1.15.1",
    "murmurhash": "^2.0.1",
    "pagedjs": "git://github.com/mildred/pagedjs.git#main",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "slate": "^0.103.0",
    "slate-history": "^0.100.0",
    "slate-react": "^0.107.0",
    "web_server": "./web_server/"
  },
  "devDependencies": {
    "@tauri-apps/cli": ">=2.0.0-beta.0",
    "@vitejs/plugin-react": "^4.3.1",
    "pagedjs-cli": "^0.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "vite-plugin-static-copy": "^1.0.6"
  }
}
